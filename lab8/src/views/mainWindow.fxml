<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainWindow">
   <children>
      <VBox layoutX="250.0" layoutY="48.0" prefHeight="299.0" prefWidth="100.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleStudentsButtonAction" prefHeight="90.0" prefWidth="100.0" text="Students">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleGradesButtonAction" prefHeight="90.0" prefWidth="100.0" text="Grades">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#handleFilterButtonAction" prefHeight="90.0" prefWidth="100.0" text="Filter">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
